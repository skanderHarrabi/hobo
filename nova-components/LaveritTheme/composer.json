{
    "name": "laverit/laverit-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Laverit\\LaveritTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laverit\\LaveritTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
