    "history": "^4.10.1",
    "html2canvas": "^1.0.0-rc.5",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-awesome-slider": "^4.0.2",
    "react-color-extractor": "^1.1.2",
    "react-dom": "^16.9.0",
    "react-infinite-scroller": "^1.2.4",
    "react-modal": "^3.11.1",
    "react-redux": "^7.1.1",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "react-socks": "^2.1.0",
    "react-tabs": "^3.1.0",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
